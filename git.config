[core] 
  autocrlf = input 
  pager = delta
[credential] 
  helper = manager
[delta] 
  navigate = true
[diff] 
  colorMoved = default
[filter "lfs"] 
  clean = git-lfs clean -- %f 
  smudge = git-lfs smudge -- %f 
  process = git-lfs filter-process 
  required = true
[help] 
  autocorrect = 1
[init] 
  defaultBranch = main
[interactive] 
  diffFilter = delta --color-only
[merge] 
  conflictstyle = diff3
