  "dart.lineLength": 120,
  "dart.maxLogLineLength": 10000,
  "dart.normalizeFileCasing": true,
  "dart.openDevTools": "flutter",
  "debug.showBreakpointsInOverviewRuler": true,
  "diffEditor.codeLens": true,
  "diffEditor.diffAlgorithm": "advanced",
  "diffEditor.experimental.collapseUnchangedRegions": true,
  "diffEditor.experimental.showMoves": true,
  "diffEditor.experimental.useVersion2": true,
  "editor.acceptSuggestionOnEnter": "smart",
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.cursorBlinking": "expand",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorStyle": "line-thin",
  "editor.definitionLinkOpensInPeek": true,
  "editor.experimental.asyncTokenization": true,
  "editor.fontFamily": "'CaskaydiaCove Nerd Font', 'Cascadia Code PL', 'CascadiaCodePL', 'MesloLGMDZ Nerd Font', Menlo, Monaco, Consolas,  'Courier New', monospace",
  "editor.fontLigatures": "'aalt', 'gdi', 'liga', 'clig', 'calt', 'falt', 'halt', 'jalt', 'ltra', 'nalt', 'palt', 'rvrn', 'salt', 'valt', 'dlig', 'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'ss07', 'ss08', 'ss09', 'ss10', 'ss11', 'ss12', 'ss13', 'ss14', 'ss15', 'ss16', 'ss17', 'ss18', 'ss19', 'ss20'",
  "editor.fontWeight": "350",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.guides.bracketPairs": "active",
  "editor.hideCursorInOverviewRuler": true,
  "editor.linkedEditing": true,
  "editor.minimap.autohide": true,
  "editor.mouseWheelZoom": true,
  "editor.rulers": [
    120
  ],
  "editor.semanticHighlighting.enabled": true,
  "editor.smoothScrolling": true,
  "editor.stablePeek": true,
  "editor.suggest.shareSuggestSelections": true,
  "editor.suggest.showStatusBar": true,
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "comment",
          "string",
          "parameter",
          "entity.name.type.class",
          "keyword",
          "constant",
          "storage.modifier",
          "storage.type.class.js"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": [
          "invalid",
          "keyword.operator",
          "constant.numeric.css",
          "keyword.other.unit.px.css",
          "constant.numeric.decimal.js",
          "constant.numeric.json"
        ],
        "settings": {
          "fontStyle": "bold italic"
        }
      }
    ]
  },
  "editor.wordWrapColumn": 120,
  "editor.wrappingStrategy": "advanced",
  "emmet.excludeLanguages": [],
  "emmet.includeLanguages": {
    "astro": "html",
    "javascript": "javascriptreact",
    "markdown": "html",
    "svelte": "html",
    "typescript": "typescriptreact",
    "vue-html": "html"
  },
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.triggerExpansionOnTab": true,
  "eslint.lintTask.enable": true,
  "eslint.packageManager": "pnpm",
  "eslint.probe": [
    "astro",
    "html",
    "html",
    "javascript",
    "javascriptreact",
    "markdown",
    "markdownreact",
    "svelte",
    "typescript",
    "typescriptreact",
    "vue"
  ],
  "eslint.validate": [
    "astro",
    "html",
    "html",
    "javascript",
    "javascriptreact",
    "markdown",
    "markdownreact",
    "svelte",
    "typescript",
    "typescriptreact",
    "vue"
  ],
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.fileNesting.enabled": true,
  "extensions.experimental.deferredStartupFinishedActivation": true,
  "extensions.webWorker": true,
  "files.autoSave": "onFocusChange",
  "files.eol": "\n",
  "files.exclude": {
    "**/.git": false
  },
  "files.insertFinalNewline": true,
  "files.simpleDialog.enable": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "git.autoStash": true,
  "git.branchRandomName.dictionary": [
    "adjectives",
    "animals",
    "colors"
  ],
  "git.branchRandomName.enable": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.mergeEditor": true,
  "git.postCommitCommand": "sync",
  "git.pruneOnFetch": true,
  "git.rebaseWhenSync": true,
  "git.terminalGitEditor": true,
  "git.useCommitInputAsStashMessage": true,
  "github.gitProtocol": "ssh",
  "githubIssues.queries": [
    {
      "label": "My Issues",
      "query": "default"
    },
    {
      "label": "Created Issues",
      "query": "author:${user} state:open repo:${owner}/${repository} sort:updated-desc"
    }
  ],
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.defaultGravatarsStyle": "retro",
  "gitlens.plusFeatures.enabled": false,
  "gitlens.showWelcomeOnInstall": false,
  "gitlens.statusBar.enabled": false,
  "gitlens.telemetry.enabled": false,
  "go.diagnostic.vulncheck": "Imports",
  "go.inlayHints.assignVariableTypes": true,
  "go.inlayHints.compositeLiteralFields": true,
  "go.inlayHints.compositeLiteralTypes": true,
  "go.inlayHints.constantValues": true,
  "go.inlayHints.functionTypeParameters": true,
  "go.inlayHints.parameterNames": true,
  "go.inlayHints.rangeVariableTypes": true,
  "go.lintTool": "revive",
  "go.toolsManagement.autoUpdate": true,
  "peacock.affectActivityBar": false,
  "peacock.affectPanelBorder": true,
  "peacock.affectTitleBar": false,
  "peacock.showColorInStatusBar": false,
  "peacock.surpriseMeOnStartup": true,
  "redhat.telemetry.enabled": false,
  "terminal.integrated.sendKeybindingsToShell": true,
  "vim.camelCaseMotion.enable": true,
  "vim.easymotion": true,
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false
  },
  "vim.highlightedyank.enable": true,
  "vim.hlsearch": true,
  "vim.incsearch": true,
  "vim.insertModeKeyBindings": [
    {
      "after": [
        "<Esc>"
      ],
      "before": [
        "j",
        "j"
      ]
    }
  ],
  "vim.leader": " ",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "after": [
        "d",
        "d"
      ],
      "before": [
        "<leader>",
        "d"
      ]
    },
    {
      "before": [
        "<C-n>"
      ],
      "commands": [
        ":nohl"
      ]
    },
    {
      "before": [
        "K"
      ],
      "commands": [
        "lineBreakInsert"
      ],
      "silent": true
    }
  ],
  "vim.smartRelativeLine": true,
  "vim.sneak": true,
  "vim.sneakUseIgnorecaseAndSmartcase": true,
  "vim.statusBarColorControl": true,
  "vim.textwidth": 120,
  "vim.useCtrlKeys": true,
  "vim.useSystemClipboard": true,
  "vim.visualstar": true,
  "window.autoDetectColorScheme": true,
  "window.closeWhenEmpty": true,
  "window.commandCenter": true,
  "workbench.colorTheme": "Rosé Pine",
