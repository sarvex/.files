{
  "breadcrumbs.symbolSortOrder": "type",
  "cSpell.userWords": [
    "aalt",
    "astro",
    "autohide",
    "calt",
    "Cascadia",
    "Caskaydia",
    "clig",
    "Consolas",
    "dlig",
    "easymotion",
    "falt",
    "highlightedyank",
    "hlsearch",
